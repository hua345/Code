<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
	<param-name>mysqlurl</param-name>
	<param-value>jdbc:mysql://localhost:3306/test</param-value>
  </context-param>
  <servlet>
    <servlet-name>ServletHello</servlet-name>
    <servlet-class>com.github.chenjianhua.ServletHello</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ThreadSafe</servlet-name>
  	<display-name>ThreadSafe</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.ThreadSafe</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletContextHello</servlet-name>
  	<display-name>ServletContextHello</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.ServletContextHello</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletContextHello2</servlet-name>
  	<display-name>ServletContextHello2</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.ServletContextHello2</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletContextParam</servlet-name>
  	<display-name>ServletContextParam</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.ServletContextParam</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReadProperties</servlet-name>
  	<display-name>ReadProperties</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.ReadProperties</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ResponseDownload</servlet-name>
  	<display-name>ResponseDownload</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.ResponseDownload</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RequestHello</servlet-name>
  	<display-name>RequestHello</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.RequestHello</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MysqlHello</servlet-name>
  	<display-name>MysqlHello</display-name>
  	<description></description>
  	<servlet-class>com.github.chenjianhua.MysqlHello</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletHello</servlet-name>
    <url-pattern>/servlethello</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  	<servlet-name>ServletContextHello</servlet-name>
  	<url-pattern>/contexthello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletContextHello2</servlet-name>
  	<url-pattern>/contexthello2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletContextParam</servlet-name>
  	<url-pattern>/contextparam</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReadProperties</servlet-name>
  	<url-pattern>/readproperties</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ResponseDownload</servlet-name>
  	<url-pattern>/responsedownload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RequestHello</servlet-name>
  	<url-pattern>/requesthello</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MysqlHello</servlet-name>
  	<url-pattern>/MysqlHello</url-pattern>
  </servlet-mapping>
</web-app>
